{
    "titl":"test blog",
    "author":"clark",
    "description":"纯手动搭建",
    "language":"zh-hans",
    "root": "./",
    "head_title": "统一规范化保存笔记",
    "head_description": "wiki",
    "head_keywords": "gitbook,文档部署",
    "gitbook": ">= 3.0.0",
    "links":{
        "siderbar":"clark-xm.xyz"
    },
    "styles": {
        "website": "./assets/website/website.css",
        "ebook": "./assets/website/ebook.css",
        "pdf": "./assets/website/pdf.css",
        "mobi": "./assets/website/mobi.css",
        "epub": "./assets/website/epub.css"
    },
    "plugins": [
        "-insert-logo",
        "code",
        "copy-code-button",
        "github" ,
        "-lunr", "-search", "search-pro",
        "-back-to-top-button","-page-toc-button","simple-page-toc",
        "chapter-fold",
        "-expandable-chapters-small","-page-treeview","-expandable-chapters",
        "advanced-emoji",
        "emphasize",
        "splitter",
        "-sharing", "sharing-plus",
        "tbfed-pagefooter",
        "page-copyright",
        "sectionx",
        
        
        
        "ancre-navigation",
        "klipse",
        "donate",
        "change_girls",
        "alerts",
        "pageview-count",
        "auto-scroll-table",
        "popup",
        "lightbox",
        "click-reveal",
        "custom-favicon",
        "-gitalk","-disqus"

        
    ],
    "pluginsConfig": {
        "insert-logo": {
          "url": "./assets/website/images/logo.png",
          "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "code": {
            "copyButtons": false
        },
        "copy-code-button":{

        },
        "github" :{
            "url": "https://github.com/Garretming/blog"
        },
        "search-pro":{},
        "back-to-top-button":{},
        "chapter-fold":{},
        "advanced-emoji":{},
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": true,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "douban", "facebook", "google", "hatenaBookmark", 
                "instapaper", "linkedin","twitter", "weibo", 
                "messenger","qq", "qzone","viber","vk","weibo",
                "pocket", "stumbleupon","whatsapp"
            ]
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy clark-xm.xyz 2019",
            "modify_label": "该文件修订时间:",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "page-copyright": {
            "description": "modified at",
            "signature": "clark",
            "wisdom": "Designer, Frontend Developer & overall web enthusiast",
            "format": "YYYY-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; clark",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "style": "normal",
            "noPowered": false
        },
        "page-treeview": {
            "copyright": "Copyright &#169; clark",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "donate": {
            "wechat": "./assets/website/images/qr-wechat-255.png",
            "alipay": "./assets/website/images/qr-alipay-255.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "change_girls" : {
            "time" : 10,
            "urls" : [
                "girlUrl1", "girlUrl2",""
            ]
        },
        "flexible-alerts": {
            "style": "callout",
            "comment": {
              "label": "Comment",
              "icon": "fa fa-comments",
              "className": "info"
            }
        },
        "favicon": "./assets/website/images/favicon.ico",
        "hide-element": {
            "elements": [".gitbook-link"]
        }

      





  


  

    }
    
        
}